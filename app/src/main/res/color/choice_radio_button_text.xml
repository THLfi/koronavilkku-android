<?xml version="1.0" encoding="utf-8"?>
<!-- sets a highlight text color for checked radio button -->

<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false" android:color="@color/textDarkGrey"/>
    <item android:state_checked="false" android:color="@color/textDarkGrey"/>
    <item android:color="@color/textBlack"/>
</selector>
